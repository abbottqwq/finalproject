# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~


GET        /                   controllers.HomeController.index()
GET        /test               controllers.TestController.test(action="testconnect")
GET        /test/:action       controllers.TestController.test(action)
POST       /initdata           controllers.HomeController.initData()
GET        /selectByComp       controllers.HomeController.selectByCompanyName(name)
GET        /selectByTime       controllers.HomeController.selectByTime(start:String, end:String, name:Option[String])

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
